  "bugs": {
    "url": "https://github.com/Luma1970/test-logotel/issues"
  },
  "homepage": "https://github.com/Luma1970/test-logotel#readme",
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^10.0.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.0.6",
    "postcss-cli": "^7.1.2"
  }
}
